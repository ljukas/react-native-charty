{
	"cli": {
		"promptToConfigurePushNotifications": false
	},
	"build": {
		"base": {
			"pnpm": "9.3.0",
			"cache": {
				"key": "turbo",
				"paths": ["../../node_modules/.cache/turbo"]
			},
			"env": {
				"EXPO_USE_FAST_RESOLVER": "true"
			}
		},
		"simulator": {
			"extends": "base",
			"distribution": "internal",
			"ios": {
				"simulator": true
			}
		},
		"development": {
			"extends": "base",
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			}
		}
	}
}
